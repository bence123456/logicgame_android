<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="logicgame.bkonecsni.com.logicgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bkonecsni.logicgame.activities.MapActivity" />
        <activity android:name="com.bkonecsni.logicgame.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bkonecsni.logicgame.activities.GamesActivity" />
        <activity android:name="com.bkonecsni.logicgame.activities.AboutActivity" />
        <activity android:name="com.bkonecsni.logicgame.activities.LevelsActivity" />
        <activity android:name="com.bkonecsni.logicgame.activities.RulesActivity"></activity>
    </application>

</manifest>